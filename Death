-- Function to position the cutscene points relative to the player
local function positionCutscenePoints()
    local newCutscenePoints = {}
    local lookVector = humanoidRootPart.CFrame.LookVector -- Direction the player is looking
    local basePosition = humanoidRootPart.Position + (lookVector * 10) -- Distance in front of the player

    for _, point in ipairs(cutscenePoints) do
        -- Calculate the offset from the first point
        local offset = point.Position - cutscenePoints[1].Position
        -- Position the point relative to the player's current position and look direction
        local newPosition = basePosition + offset
        table.insert(newCutscenePoints, CFrame.new(newPosition, newPosition + lookVector))
    end

    return newCutscenePoints
end

-- Function to play the cutscene
local function playCutscene(cutscenePoints)
    for _, point in ipairs(cutscenePoints) do
        camera.CFrame = point
        RunService.RenderStepped:Wait() -- Adjust this time for how fast the cutscene should progress
    end
end

-- Example usage: Adjust cutscene points based on player view and play the cutscene
local adjustedCutscenePoints = positionCutscenePoints()
playCutscene(adjustedCutscenePoints)
