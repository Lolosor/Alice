Anim3.AnimationId = "rbxassetid://14542032218"
local AnimTrack3 = humanoid:LoadAnimation(Anim3)

-- Play the first animation
AnimTrack1:Play()

-- Wait for 1.7 seconds, then stop the first animation and play the second animation
delay(1.8, function()
    AnimTrack1:Stop()
    AnimTrack2:Play()
end)

-- Wait for 2.7 seconds, then stop the second animation and play the third animation
delay(1.8, function()
    AnimTrack2:Stop()
    AnimTrack3:Play()
            end)
  	end    
})
