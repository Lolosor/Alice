Tab:AddButton({
    Name = "M1 / 4",
    Callback = function()
        local player = workspace:FindFirstChild("Live"):FindFirstChild("Weakest Dummy")
        if not player then
            warn("Player model not found")
            return
        end

        local humanoid = player:FindFirstChild("Humanoid")
        if not humanoid then
            warn("Humanoid not found")
            return
        end

        local anim2 = Instance.new("Animation")
        anim2.AnimationId = "rbxassetid://17889290569"

        local playAnim2 = humanoid:LoadAnimation(anim2)
        playAnim2:Play()
    end
})
