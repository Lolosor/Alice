animation1.AnimationId = "rbxassetid://" .. firstAnimationId
local animationTrack1 = humanoid:LoadAnimation(animation1)

local animation2 = Instance.new("Animation")
animation2.AnimationId = "rbxassetid://" .. secondAnimationId
local animationTrack2 = humanoid:LoadAnimation(animation2)

-- Function to teleport the character forward
local function teleportForward(distance)
    humanoidRootPart.CFrame = humanoidRootPart.CFrame * CFrame.new(0, 0, -distance)
end

-- Play the first animation
animationTrack1:Play()

-- Wait for the first animation to finish, then teleport and play the second animation
animationTrack1.Stopped:Wait()

-- Teleport the character forward by 100 studs
wait()
teleportForward(100)

-- Play the second animation
animationTrack2:Play()
      end
})
