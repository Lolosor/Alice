Tab:AddButton({
    Name = "Twinblade Rush (Start Up)",
    Callback = function()
            local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")

-- Clear existing animations
for _, animTrack in pairs(humanoid:GetPlayingAnimationTracks()) do
    animTrack:Stop()
end

-- First animation
local Anim1 = Instance.new("Animation")
Anim1.AnimationId = "rbxassetid://13632347366"  -- First animation ID
local AnimTrack1 = humanoid:LoadAnimation(Anim1)

-- Play the first animation
AnimTrack1:Play()

-- Function to teleport forward
local function teleportForward()
    if humanoidRootPart then
        for i = 1, 46 do  -- 4.6 seconds / 0.1 seconds = 46 iterations
            local lookVector = humanoidRootPart.CFrame.LookVector
            humanoidRootPart.CFrame = humanoidRootPart.CFrame + lookVector * 2  -- Move 2 studs forward
            wait()  -- Wait for 0.1 seconds
        end
    end
end

-- Start the teleport after a short delay
wait()
teleportForward()

-- Stop the first animation after teleporting
wait(2.6)
AnimTrack1:Stop()  
      end
})
