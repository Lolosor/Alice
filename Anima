Anim1.AnimationId = "rbxassetid://13643152947"  -- First animation ID
local AnimTrack1 = humanoid:LoadAnimation(Anim1)

-- Second animation (looped)
local Anim2 = Instance.new("Animation")
Anim2.AnimationId = "rbxassetid://13634395775"  -- Second animation ID
local AnimTrack2 = humanoid:LoadAnimation(Anim2)
AnimTrack2.Looped = true  -- Set to loop

-- Third animation
local Anim3 = Instance.new("Animation")
Anim3.AnimationId = "rbxassetid://13639700348"  -- Third animation ID
local AnimTrack3 = humanoid:LoadAnimation(Anim3)

-- Function to teleport forward
local function teleportForward(duration, distance, interval)
    if humanoidRootPart then
        local startTime = tick()
        while tick() - startTime < duration do
            local lookVector = humanoidRootPart.CFrame.LookVector
            humanoidRootPart.CFrame = humanoidRootPart.CFrame + lookVector * distance
            wait(interval)  -- Wait for the specified interval
        end
    end
end

-- Play the first animation
AnimTrack1:Play()

-- Wait for 0.6 seconds before starting the teleport for the first animation
wait(0.4)
teleportForward(2.4, 3, 0)  -- Teleport for 2.4 seconds, 3 studs per move, 0 second intervals

-- Play the second animation
AnimTrack2:Play()

-- Wait for 0.6 seconds before starting the teleport for the second animation
wait(0)
teleportForward(1.4, 3, 0)  -- Teleport for 1.4 seconds, 3 studs per move, 0 second intervals

-- Stop the second animation after 2 seconds
wait()
AnimTrack2:Stop()

-- Play the third animation
AnimTrack3:Play()

-- Wait for 0.5 seconds before teleporting for the third animation
wait(0.5)

-- Teleport 40 studs forward only once
if humanoidRootPart then
    local lookVector = humanoidRootPart.CFrame.LookVector
    humanoidRootPart.CFrame = humanoidRootPart.CFrame + lookVector * 40
end

-- Stop the third animation after it has played for 9 seconds
wait(9)
AnimTrack3:Stop()
      end
})
