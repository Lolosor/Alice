-- M1 Animations Section
local Tab = Window:NewTab("M1 Animations")

-- Helper function to create M1 animation sections
local function createM1Section(title, animations)
    local Section = Tab:NewSection(title)
    for _, animation in ipairs(animations) do
        Section:NewButton(animation.name, "", function()
            playAnimation(animation.id)
        end)
    end
end

createM1Section("The Strongest Hero", {
    {name = "M1 / 1", id = "10469493270"},
