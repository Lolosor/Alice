local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Alice Hub", "Synapse")

-- Function to ensure the humanoid is ready
local function getHumanoid()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    return character:WaitForChild("Humanoid")
end

local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("Characters FE")

-- Define buttons with unique URLs
local buttons = {
    {name = "Saitama To Gojo", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/Saitama%20to%20gojo"},
    {name = "Saitama To M1 Master", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/M1%20Master"},
    {name = "Garou To Combat", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/Combat"},
    {name = "Garou To Sukuna", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/Sukuna"},
    {name = "Garou To Cosmic Garou", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/Cosmic%20Garou%20Glitcher"},
    {name = "Garou To Goku", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/Goku"},
    {name = "Saitama To KJ", url = "https://raw.githubusercontent.com/Lolosor/Alice/main/KJ"}
}

-- Create buttons dynamically
for _, button in ipairs(buttons) do
    Section:NewButton(button.name, "", function()
        loadstring(game:HttpGet(button.url))()
    end)
end

local Tab = Window:NewTab("Spawn Animations")
local Section = Tab:NewSection("Spawn Animations")

-- Define spawn animations with unique AnimationIds
local spawnAnimations = {
    {name = "The Strongest Hero", id = "rbxassetid://15957366251"},
    {name = "Hero Hunter", id = "rbxassetid://15957376722"},
    {name = "Destructive Cyborg", id = "rbxassetid://15957374019"},
    {name = "Deadly Ninja", id = "rbxassetid://15957361339"},
    {name = "Brutal Demon", id = "rbxassetid://15957371124"},
    {name = "Bladd Master", id = "rbxassetid://15983615423"},
    {name = "Wild Psychic", id = "rbxassetid://16136144568"},
    {name = "Martial Artist", id = "rbxassetid://18435303746"},
    {name = "KJ", id = "rbxassetid://17325160621"}
}

-- Create buttons dynamically
for _, animation in ipairs(spawnAnimations) do
    Section:NewButton(animation.name, "", function()
        local humanoid = getHumanoid()
        local anim = Instance.new("Animation")
        anim.AnimationId = animation.id
        local playAnim = humanoid:LoadAnimation(anim)
        playAnim:Play()
    end)
end

