    loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)

miscSection:NewTextBox("WalkSpeed", "Enter the desired speed", function(value)
    local speed = tonumber(value)
    if speed then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.WalkSpeed = speed
    else
        warn("Invalid input for WalkSpeed.")
    end
end)

miscSection:NewTextBox("JumpPower", "Enter the desired jump power", function(value)
    local jumpPower = tonumber(value)
    if jumpPower then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        humanoid.UseJumpPower = true
        humanoid.JumpPower = jumpPower
    else
        warn("Invalid input for JumpPower.")
    end
end)

