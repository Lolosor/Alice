local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()
local Window = Library.CreateLib("Nuh uh", "Synapse")

-- Characters FE Tab
local CharactersTab = Window:NewTab("Main")
local CharactersSection = CharactersTab:NewSection("Characters FE")

local characters = {
    {name = "Saitama To Gojo", url = "private"},
    {name = "Saitama To M1 Master", url = "private"},
    {name = "Garou To Combat", url = "private"},
    {name = "Garou To Sukuna", url = "private"},
    {name = "Garou To Cosmic Garou", url = "private"},
    {name = "Garou To Goku", url = "private"},
    {name = "Saitama To KJ", url = "private"},
}

for _, char in ipairs(characters) do
    CharactersSection:NewButton(char.name, "", function()
        loadstring(game:HttpGet(char.url))()
    end)
end

-- Misc Section
local MiscTab = Window:NewTab("Misc")
local MiscSection = MiscTab:NewSection("Misc")

MiscSection:NewButton("Infinite Yield", "OP FE admin commands", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

MiscSection:NewButton("Dex Explorer V3", "See what's inside the game", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Babyhamsta/RBLX_Scripts/main/Universal/BypassedDarkDexV3.lua", true))()
end)

MiscSection:NewButton("Animation Logger", "Detects current played animation", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Lolosor/Alice/main/Animation%20Logger"))()
end)

MiscSection:NewButton("Aimbot", "Locks at the nearest player's head", function()
    loadstring(game:HttpGet("https://pastebin.com/raw/1Gp9c57U"))()
end)

MiscSection:NewButton("Keyboard", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt", true))()
end)

MiscSection:NewTextBox("WalkSpeed", "Enter the speed you want", function(value)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    local speed = tonumber(value)
    
    if speed then
        humanoid.WalkSpeed = speed
    else
        warn("Please enter a valid number")
    end
end)

MiscSection:NewTextBox("JumpPower", "Enter the jump power you want", function(value)
    local jumpPower = tonumber(value)
    
    if jumpPower then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        humanoid.JumpPower = jumpPower
        humanoid.UseJumpPower = true
        print("Jump Power set to", jumpPower)
    else
        warn("Please enter a valid number for Jump Power")
    end
end)

