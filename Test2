-- 6. Movesets Tab
local movesetTab = Window:NewTab("Movesets")

-- Helper Function for Sequenced Animations
local function playAnimationSequence(animationIds, delays)
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoid = character:WaitForChild("Humanoid")

    -- Stop all current animations
    for _, track in pairs(humanoid:GetPlayingAnimationTracks()) do
        track:Stop()
    end

    local index = 1
    local function playNext()
        if index <= #animationIds then
            local anim = Instance.new("Animation")
            anim.AnimationId = "rbxassetid://" .. animationIds[index]
            local animTrack = humanoid:LoadAnimation(anim)
            animTrack:Play()
            index = index + 1
            if delays[index - 1] then
                delay(delays[index - 1], playNext)
            end
        end
    end

    playNext()
end

-- The Strongest Hero Movesets
local heroSection = movesetTab:NewSection("The Strongest Hero")

heroSection:NewButton("Normal Punch", "", function()
    playAnimation("10468665991")
end)

heroSection:NewButton("Consecutive Punches", "", function()
    playAnimation("10466974800")
end)

heroSection:NewButton("Shove", "", function()
    playAnimation("10471336737")
end)

heroSection:NewButton("Uppercut", "", function()
    playAnimation("12510170988")
end)

-- Hero Hunter Movesets
local hunterSection = movesetTab:NewSection("Hero Hunter")

-- Flowing Water Moves
hunterSection:NewButton("Flowing Water (Miss)", "", function()
    playAnimation("12272894215")
end)

hunterSection:NewButton("Flowing Water (Full)", "", function()
    playAnimationSequence(
        {"12272894215", "12273188754"},
        {0.6}  -- Delay before the second animation
    )
end)

hunterSection:NewButton("Flowing Water (Finisher)", "", function()
    playAnimationSequence(
        {"12272894215", "14374357351"},
        {0.6}
    )
end)

