local Tab = Window:NewTab("Misc")
local Section = Tab:NewSection("Misc")
Section:NewTextBox("WalkSpeed", "Enter the speed you want", function(value)
    local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

-- Function to continuously enforce WalkSpeed
local function enforceWalkSpeed()
    while true do
        -- Ensure the Humanoid is valid
        if humanoid then
            humanoid.WalkSpeed = value
        end
        wait(0.1) -- Adjust the frequency as needed
    end
end

-- Connect to the CharacterAdded event to handle respawns
player.CharacterAdded:Connect(function(char)
    humanoid = char:WaitForChild("Humanoid")
end)

-- Start the enforcement loop
enforceWalkSpeed()
end)
Section:NewTextBox("JumpPower", "Enter the jump power you want", function(value)
    -- Convert the value from string to number
    local newJumpPower = tonumber(value)
    
    -- Ensure newJumpPower is a valid number
    if newJumpPower then
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        
        -- Function to continuously update JumpPower
        local function updateJumpPower()
            while true do
                if newJumpPower then
                    humanoid.JumpPower = newJumpPower
                    humanoid.UseJumpPower = true
                    print("Jump Power set to", newJumpPower)
                else
                    print("Please enter a valid number for Jump Power")
                end
                wait(0.1) -- Adjust the wait time if needed
            end
        end

        -- Start updating JumpPower in a coroutine
        coroutine.wrap(updateJumpPower)()
    else
        -- Notify the player if the input is invalid
        print("Please enter a valid number for Jump Power")
    end

    -- Optionally handle changes to the textbox value
    Section:OnValueChanged(function(newValue)
        newJumpPower = tonumber(newValue)
    end)
end)
