local Tab = Window:NewTab("Movesets")
local Section = Tab:NewSection("The Strongest Hero")

Section:NewButton("Normal Punch", "", function()
    playAnimation("15957366251")
end)
Section:NewButton("Normal Punch", "", function()
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    
    -- Ensure Humanoid is available
    local humanoid = character:WaitForChild("Humanoid")
    
    -- Create and load animation
    local anim2 = Instance.new("Animation")
    anim2.AnimationId = "rbxassetid://10468665991"
    local playAnim2 = humanoid:LoadAnimation(anim2)
    
    -- Play the animation
    playAnim2:Play()
end)
