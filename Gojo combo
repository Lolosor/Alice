humanoid.AnimationPlayed:Connect(onAnimationPlayed)

local animationId = 10479335397

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")

local function onAnimationPlayed(animationTrack)
    if animationTrack.Animation.AnimationId == "rbxassetid://" .. animationId then
        -- First Animation Logic
        local p = game.Players.LocalPlayer
        local Humanoid = p.Character:WaitForChild("Humanoid")

        for _, animTrack in pairs(Humanoid:GetPlayingAnimationTracks()) do
            animTrack:Stop()
        end

        local AnimAnim1 = Instance.new("Animation")
        AnimAnim1.AnimationId = "rbxassetid://13643152947"
        local Anim1 = Humanoid:LoadAnimation(AnimAnim1)

        -- Start the first animation at a specific time and adjust speed
        local startTime1 = 0.8
        Anim1:Play()
        Anim1:AdjustSpeed(1)
        Anim1.TimePosition = startTime1
        Anim1:AdjustSpeed(1)

        -- Stop the first animation after a delay
        delay(0.86, function()
            Anim1:Stop()
        end)

        -- Wait for 1 second before starting the second animation
        wait(0.86)

        -- Second Animation Logic
        local AnimAnim2 = Instance.new("Animation")
        AnimAnim2.AnimationId = "rbxassetid://18181589384"
        local Anim2 = Humanoid:LoadAnimation(AnimAnim2)

        -- Start the second animation at a specific time and adjust speed
        local startTime2 = 1.3
        Anim2:Play()
        Anim2:AdjustSpeed(1)
        Anim2.TimePosition = startTime2
        Anim2:AdjustSpeed(0.6)

        -- Stop the second animation after a delay
        delay(0.46, function()
            Anim2:Stop()
        end)
    end
end

humanoid.AnimationPlayed:Connect(onAnimationPlayed)
